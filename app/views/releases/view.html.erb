<%= stylesheet_link_tag "releases", :media => "all" %>

<% Settings.projects.each do |project| %>
  <section>
    <div class="tile is-ancestor">
       <%= render partial:  "medium_title_box" , locals: { project: project } %>
       <% project.environments.each do |environment| %>
           <%= render partial:  "medium_data_box" , locals: { project: project , environment: environment } %>
       <% end %>
    </div>
  </section>
 <% end %>

